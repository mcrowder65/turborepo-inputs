{
  "name": "rs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "shared": "*"
  },
  "scripts": {
    "turbo:cwd": "turbo run jest --cwd=../.. --scope='rs'",
    "turbo:yarncwd": "yarn --cwd ../.. turbo run jest --scope='rs'",
    "start": "react-app-rewired start",
    "dev": "yarn start",
    "build": "react-app-rewired build",
    "jest": "jest",
    "test": "jest",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {}
}
